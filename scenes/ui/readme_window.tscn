[b][color=orange]Example 1: Basic Navigation[/color][/b]
[color=gray]while not car.at_end():
    if car.front_road():
        car.go()
    elif car.left_road():
        car.turn(\\\"left\\\")
        car.go()
    elif car.right_road():
        car.turn(\\\"right\\\")
        car.go()
    else:
        car.stop()[/color]

[b][color=orange]Example 2: Stoplight Handling[/color][/b]
[color=gray]def check_stoplight():
    if stoplight.is_red():
        car.stop()
        return False
    return True

while not car.at_end():
    if check_stoplight():
        if car.front_road():
            car.go()[/color]

[b][color=orange]Example 3: Crash Avoidance[/color][/b]
[color=gray]def avoid_obstacles():
    if car.front_crash() or car.front_car():
        if car.left_road():
            car.turn(\\\"left\\\")
            car.go()
        elif car.right_road():
            car.turn(\\\"right\\\")
            car.go()
        else:
            car.stop()
        return True
    return False

while not car.at_end():
    if not avoid_obstacles():
        if car.front_road():
            car.go()
        else:
            car.stop()[/color]

[b][color=orange]Example 4: Dead End Detection[/color][/b]
[color=gray]while not car.at_end():
    if car.dead_end():
        print(\\\"Reached dead end!\\\")
        break
    if car.front_road():
        car.go()
    else:
        car.turn(\\\"right\\\")[/color]


